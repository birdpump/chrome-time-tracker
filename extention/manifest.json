{
  "manifest_version": 3,
  "name": "Time Tracker",
  "version": "1.0",
  "description": "Tracks time on sites",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Time Tracker"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
